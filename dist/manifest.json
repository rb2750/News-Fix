{
    "name": "NewsSitesFix",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Fixes a bug that prevents you from reading most news site articles.",
    "homepage_url": "https://www.thestar.co.uk/",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "activeTab"
    ],
    "content_security_policy": "default-src 'self';",
    "content_scripts": [
      {
        "matches": [
          "https://*/*"
        ],
        "js": [
          "js/content.js"
        ]
      }
    ]
  }
  
